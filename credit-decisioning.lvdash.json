{
  "datasets": [
    {
      "name": "3e00a0f3",
      "displayName": "dbdemos HLS - Patients by State",
      "query": "SELECT NVL(state, 'XX' ) state_abbr, \n    count(*) as Num_Persons_count\nfrom `main__build`.`dbdemos_hsl_readmission`.patients p\nWHERE p.BIRTHDATE BETWEEN :start_date AND :end_date\nGROUP BY state_abbr\nORDER BY 1;",
      "parameters": [
        {
          "displayName": "start_date",
          "keyword": "start_date",
          "dataType": "DATE",
          "defaultSelection": {
            "values": {
              "dataType": "DATE",
              "values": [
                {
                  "value": "2018-04-01T00:00:00.000"
                }
              ]
            }
          }
        },
        {
          "displayName": "end_date",
          "keyword": "end_date",
          "dataType": "DATE",
          "defaultSelection": {
            "values": {
              "dataType": "DATE",
              "values": [
                {
                  "value": "2024-04-25T00:00:00.000"
                }
              ]
            }
          }
        }
      ]
    },
    {
      "name": "c53be963",
      "displayName": "dbdemos HLS - patient summary",
      "query": "SELECT c.concept_name gender, p.day_of_birth, p.month_of_birth, p.year_of_birth, COUNT(person_id) AS num_persons_count\nFROM `main__build`.`dbdemos_hsl_readmission`.person p\nLEFT JOIN `main__build`.`dbdemos_hsl_readmission`.concept c ON c.concept_id = p.gender_concept_id\nWHERE birth_datetime BETWEEN :start_date AND :end_date \nGROUP BY c.concept_name, p.day_of_birth, p.month_of_birth, p.year_of_birth\nORDER BY p.year_of_birth ASC",
      "parameters": [
        {
          "displayName": "start_date",
          "keyword": "start_date",
          "dataType": "DATE",
          "defaultSelection": {
            "values": {
              "dataType": "DATE",
              "values": [
                {
                  "value": "2021-04-01T00:00:00.000"
                }
              ]
            }
          }
        },
        {
          "displayName": "end_date",
          "keyword": "end_date",
          "dataType": "DATE",
          "defaultSelection": {
            "values": {
              "dataType": "DATE",
              "values": [
                {
                  "value": "2024-04-29T00:00:00.000"
                }
              ]
            }
          }
        }
      ]
    },
    {
      "name": "4b60505f",
      "displayName": "dbdemos HLS - Total Patients",
      "query": "SELECT COUNT(person_id) AS num_persons_count\nFROM `main__build`.`dbdemos_hsl_readmission`.person\nWHERE birth_datetime BETWEEN :start_date AND :end_date",
      "parameters": [
        {
