        {
            "id": "96eba5c8-56cb-4415-8ff4-b96e8e5e600b",
            "name": "dbdemos HLS - patient summary",
            "description": null,
            "query": "SELECT c.concept_name gender, p.day_of_birth, p.month_of_birth, p.year_of_birth, COUNT(person_id) AS num_persons_count\nFROM dbdemos.hls_patient_readmission.person p\nLEFT JOIN dbdemos.hls_patient_readmission.concept c ON c.concept_id = p.gender_concept_id\nWHERE birth_datetime BETWEEN '{{ from }}' AND '{{ to }}'\nGROUP BY c.concept_name, p.day_of_birth, p.month_of_birth, p.year_of_birth\nORDER BY p.year_of_birth ASC",
